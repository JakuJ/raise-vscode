{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "RSL",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.rsl",
				"match": "\\b(scheme|class|type|value|axiom|end)\\b"
			},
			{
				"name": "keyword.operator.bounded.rsl",
				"match": "\\~isin\\b|-inflist\\b|-list\\b|\\b(inter|exists|union|all|always|is|isin)\\b"
			},
			{
				"name": "keyword.operator.unbound.rsl",
				"match": "<<=|>>=|><|(\\|\\|)|(\\|=\\|)|\\*\\*|/\\\\|>=|<=|-m->|-\\~->|->|#|:-|\\+\\+|(\\|\\^\\|)|\/|<<|>>|<\\.|-\\~m->|-\\\\|~=|\\+>|!!|=>|<->|\\.>"
			},
			{
				"name": "support.type.rsl",
				"match": "\\b(Int|Nat)\\b"
			}
		]
		},
		"strings": {
			"name": "string.quoted.double.rsl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.rsl",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.rsl"
}